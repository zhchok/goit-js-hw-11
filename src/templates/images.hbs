{{#each this}}
	<a href="{{largeImageURL}}" class="photo-link">
		<div class="photo-card">
			<div class="img-thumb">
				<img src="{{webformatURL}}" alt="{{tags}}" width loading="lazy" />
			</div>
			<div class="info">
				<p class="info-item">
					<b>
						Likes:
						<br />
						{{likes}}
					</b>
				</p>
				<p class="info-item">
					<b>
						Views:
						<br />
						{{views}}
					</b>
				</p>
				<p class="info-item">
					<b>
						Comments:
						<br />
						{{comments}}
					</b>
				</p>
				<p class="info-item">
					<b>
						Downloads:
						<br />
						{{downloads}}
					</b>
				</p>
			</div>
		</div>
	</a>
{{/each}}